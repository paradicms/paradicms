{
  "author": "Minor Gordon",
  "name": "@paradicms/filters",
  "description": "Paradicms filters library",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "14.17.32",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "rimraf": "3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist",
    "rebuild": "rimraf dist && tsc -b",
    "test": "env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha -r ts-node/register 'test/**/*.ts'",
    "test-watch": "env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha -r ts-node/register -w 'test/**/*.ts'",
    "watch": "tsc -w --preserveWatchOutput"
  },
  "types": "dist/index.d.ts",
  "version": "1.0.5"
}
