{
  "author": "Minor Gordon",
  "name": "@paradicms/test",
  "description": "Paradicms testing library",
  "dependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "14.17.32",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "mocha-profiler": "1.0.0",
    "nyc": "15.1.0",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "copyfiles": "2.4.1",
    "rimraf": "3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "scripts": {
    "build": "copyfiles --error -u 1 src/synthetic_data.trig dist && tsc -b",
    "clean": "rimraf dist",
    "rebuild": "rimraf dist && copyfiles --error -u 1 src/synthetic_data.trig dist && tsc -b",
    "watch": "copyfiles --error -u 1 src/synthetic_data.trig dist && tsc -w --preserveWatchOutput"
  },
  "types": "dist/index.d.ts",
