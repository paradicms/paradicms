{
  "author": "Minor Gordon",
  "name": "@paradicms/models",
  "description": "Paradicms models library",
  "dependencies": {
    "@paradicms/vocabularies": "1.0.5",
    "dayjs": "1.10.7",
    "ts-mixer": "6.0.0",
    "typescript-memoize": "1.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "14.17.32",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "mocha-profiler": "1.0.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.21",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.4"
  },
  "license": "GPL-3.0-only",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ]
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist",
    "rebuild": "rimraf dist && tsc -b",
    "test": "env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha -r ts-node/register 'test/**/*.ts'",
    "test-coverage": "env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' nyc mocha -r source-map-support/register -r ts-node/register 'test/**/*.ts'",
    "test-profile": "env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha -r ts-node/register -r mocha-profiler 'test/**/DataSubsetter.test.ts'",
    "test-watch": "env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha -r ts-node/register -w 'test/**/*.ts'",
    "watch": "tsc -w --preserveWatchOutput"
  },
  "types": "dist/index.d.ts",
  "version": "1.0.5"
}
